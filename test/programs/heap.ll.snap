; ModuleID = 'whacko'
source_filename = "whacko"
target triple = "wasm32-wasi"

define void @_start() {
entry:
  %"alloca~0" = alloca i32, align 4
  %"250~inst" = tail call ptr @malloc(i32 mul (i32 ptrtoint (ptr getelementptr (i8, ptr null, i32 1) to i32), i32 100))
  %"251~inst" = ptrtoint ptr %"250~inst" to i32
  store i32 %"251~inst", ptr %"alloca~0", align 4
  %"253~inst" = load i32, ptr %"alloca~0", align 4
  %"254~inst" = inttoptr i32 %"253~inst" to ptr
  tail call void @free(ptr %"254~inst")
  ret void
}

declare noalias ptr @malloc(i32) #0

declare void @free(ptr) #1

attributes #0 = { "alloc-family"="malloc" "allockind"="alloc,zeroed" "allocsize"="0" }
attributes #1 = { "alloc-family"="malloc" "allockind"="free" }
