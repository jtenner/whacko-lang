{
  "name": "whacko",
  "scopeName": "source.whacko",
  "fileTypes": [
    ".wo"
  ],
  "patterns": [
    {
      "include": "#comments"
    },
    {
      "name": "keyword.control.whacko",
      "match": "\\b(as|async|await|break|class|const|constructor|continue|declare|else|export|extends|false|final|fn|from|get|grab|held|hold|if|import|let|new|null|private|return|set|super|this|true|type|while|yield)\\b"
    },
    {
      "name": "string.quoted.double.whacko",
      "begin": "\"",
      "end": "[^\"\\r\\n]\\\\\\.\""
    }
  ],
  "repository": {
    "comments": {
      "patterns": [
        {
          "name": "comment.block.whacko",
          "begin": "/\\*",
          "beginCaptures": {
            "0": {
              "name": "punctuation.definition.comment.whacko"
            }
          },
          "end": "\\*/",
          "endCaptures": {
            "0": {
              "name": "punctuation.definition.comment.whacko"
            }
          }
        },
        {
          "begin": "//",
          "beginCaptures": {
            "1": {
              "name": "punctuation.whitespace.comment.leading.whacko"
            }
          },
          "end": "(?=$)",
          "name": "comment.line.whacko"
        }
      ]
    }
  }
}