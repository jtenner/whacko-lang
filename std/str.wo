export class str {
  constructor() {}

  @operator("[]") __get(offset: usize): u8 {
    return memory.load<u8>(memory.ptr(this) + usize(8) + offset);
  }

  @operator("[]=") __set(offset: usize, value: u8): void {}

  toString(): String {
    const str = new String();
    str.appendStr(this);
    return str;
  }
}
