export class str {
  ptr: usize;
  size: usize;

  constructor(ptr: usize, size: usize) {
    this.ptr = ptr;
    this.size = size;
  }

  @operator("[]") __get(offset: usize): u8 {
    return memory.load<u8>(this.ptr + offset);
  }

  toString(): String {
    const str = new String();
    str.appendStr(this);
    return str;
  }
}
